    "@trpc/react-query": "^10.9.0",
    "@trpc/server": "^10.9.0",
    "bson-objectid": "^2.0.4",
    "date-fns": "^2.29.3",
    "msw-trpc": "^1.2.0",
    "next": "13.1.6",
    "next-auth": "^4.19.0",
    "node-fetch": "^2.6.9",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "^1.9.1",
    "trpc-panel": "^1.2.8",
    "tsx": "^3.12.3",
    "xlsx": "^0.18.5",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
