{
  "name": "cityrays",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "build": "next build --debug",
    "dev": "next dev",
    "test": "vitest",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "start": "next start",
    "db:up": "docker-compose up -d",
    "db:down": "docker-compose down",
    "db:sync": "prisma db push",
    "db:seed": "node prisma/seed.mjs",
    "db:init": "docker-compose exec mongo mongo --eval \"rs.initiate({_id: 'rs0', members: [{_id: 0, host: 'localhost:27017'}]});\"",
    "upload": "tsx ./script/upload.ts ./cityrays-files/*.xlsx"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "^4.13.0",
